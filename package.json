{
  "name": "@jfdevelops/multi-step-form",
  "version": "1.0.0-alpha.5",
  "description": "An opinionated type safe solution to creating multi step forms.",
  "type": "module",
  "scripts": {
    "setup:all": "pnpm i && pnpm -r build",
    "setup:packages": "pnpm i && pnpm --filter './packages/*' -r build",
    "reload": "pnpm build || pnpm i",
    "watch": "pnpm -r --parallel run watch",
    "build": "pnpm -r build",
    "build:core": "pnpm -r --filter=@jfdevelops/multi-step-form run build",
    "build:react": "pnpm -r --filter=@jfdevelops/react-multi-step-form run build",
    "build:packages": "pnpm run build:core && pnpm run build:react",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "test:packages": "pnpm -r --filter \"@jfdevelops/*\" test",
    "test:core": "pnpm -r --filter=@jfdevelops/multi-step-form test",
    "test:react": "pnpm -r --filter=@jfdevelops/react-multi-step-form test",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.18.2",
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.5.0",
    "husky": "^9.1.7",
    "typescript": "^5.9.2",
    "vite": "^7.1.11"
  },
  "pnpm": {
    "overrides": {
      "typescript": "5.9.2"
    }
  }
}
